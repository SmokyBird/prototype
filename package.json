{
  "name": "la-liga",
  "version": "2.0.0-alpha.1",
  "description": "A football manager-type game for e-sports",
  "main": "main.bundle.js",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lemonpole/la-liga#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/la-liga.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/la-liga/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "e-sport",
    "simulator"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production node -r babel-register ./webpack/compiler.js",
    "flow": "flow",
    "flow-typed:install": "flow-typed install --overwrite",
    "lint": "eslint --ext .js,.jsx ./",
    "start": "cross-env NODE_ENV=development node -r babel-register ./webpack/compiler.js",
    "start:electron": "cross-env NODE_ENV=development electron ./main.bundle.js",
    "squad-updater": "node -r babel-register ./renderer-process/lib/scraper-factory/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "build": {
    "appId": "com.electron.laliga",
    "mac": {
      "category": "public.app.category.games",
      "identity": "null"
    },
    "dmg": {
      "title": "${productName}"
    },
    "nsis": {
      "uninstallDisplayName": "${productName}"
    },
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "main.bundle.js",
      "dist/renderer/windows/**/*"
    ],
    "publish": {
      "provider": "github",
      "releaseType": "release"
    }
  },
  "dependencies": {
    "adjective-animal": "1.1.3",
    "antd": "3.11.6",
    "chalk": "2.4.1",
    "cheerio": "0.19.0",
    "cloudscraper": "1.5.0",
    "countries-list": "2.3.2",
    "cuid": "2.1.4",
    "duel": "3.2.0",
    "duel-names": "1.2.0",
    "electron-is": "3.0.0",
    "electron-log": "2.2.17",
    "electron-updater": "4.0.6",
    "ffa": "3.2.2",
    "ffa-tb": "1.2.1",
    "glob": "7.1.3",
    "group": "1.0.2",
    "groupstage": "3.1.1",
    "groupstage-tb": "1.3.1",
    "groupstage-tb-duel": "1.2.0",
    "lodash": "4.17.11",
    "masters": "3.1.0",
    "minimist": "1.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-particles-js": "2.4.2",
    "react-ratings-declarative": "3.4.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.5.2",
    "roundrobin": "1.0.3",
    "tiebreaker": "3.1.0",
    "tournament": "3.2.0",
    "tourney": "1.2.1",
    "validator": "10.10.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "babel-plugin-import": "1.11.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "1.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "electron": "4.0.0",
    "electron-builder": "20.38.4",
    "electron-rebuild": "1.8.2",
    "empty": "0.10.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-react": "7.10.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "flow-bin": "0.76.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.3.0",
    "mock-fs": "4.5.0",
    "node-sass": "4.9.1",
    "postcss-loader": "2.1.5",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.15.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "lcov",
      "json-summary"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|jpg|png)$": "<rootDir>/__test__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "main-process/lib"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 67,
        "lines": 70,
        "statements": 70
      }
    },
    "globals": {
      "__DEV__": true
    },
    "notify": true,
    "collectCoverageFrom": [
      "main-process/lib/**/*.{js,jsx}"
    ]
  }
}
