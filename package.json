{
  "name": "la-liga",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "description": "A football manager-type game for e-sports",
  "homepage": "https://github.com/lemonpole/la-liga#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/la-liga.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/la-liga/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "e-sport",
    "simulator"
  ],
  "main": "main.js",
  "dependencies": {
    "electron": "1.6.10",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.16.3",
    "chalk": "1.1.3",
    "cheerio": "0.19.0",
    "cloudscraper": "1.4.0",
    "css-loader": "0.26.2",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.30.3",
    "eslint-plugin-flowtype-errors": "3.0.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.4.1",
    "express": "4.14.0",
    "file-loader": "0.10.1",
    "flow-bin": "0.41.0",
    "flow-typed": "2.0.0",
    "glob": "7.1.1",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "node-sass": "3.13.0",
    "postcss-loader": "1.3.3",
    "react-transform-hmr": "1.0.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "scripts": {
    "build:main": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/webpack-electron.js --progress --profile --colors",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint": "eslint --ext .js,.jsx ./",
    "start": "NODE_ENV=production electron ./dist/main.bundle.js",
    "start:dev": "npm run start:dev-server -- --dev-electron",
    "start:dev-electron": "NODE_ENV=development electron -r babel-register -r babel-polyfill ./main",
    "start:dev-server": "NODE_ENV=development node -r babel-register ./webpack/server.js",
    "squad-updater": "node -r babel-register ./scripts/squad-updater/index.js"
  }
}
