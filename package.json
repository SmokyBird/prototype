{
  "name": "la-liga",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "main": "main.js",
  "description": "A football manager-type game for e-sports",
  "homepage": "https://github.com/lemonpole/la-liga#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/la-liga.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/la-liga/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "e-sport",
    "simulator"
  ],
  "scripts": {
    "build:main": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/webpack-electron.js --progress --profile --colors",
    "db:sequelize": "sequelize $@",
    "db:console": "sqlite3 $@",
    "flow": "flow",
    "flow-typed:install": "flow-typed install --overwrite",
    "lint": "eslint --ext .js,.jsx ./",
    "start": "NODE_ENV=production electron ./dist/main.bundle.js",
    "start:dev": "npm run start:dev-server -- --dev-electron",
    "start:dev:console": "npm run start:dev-server -- --dev-electron --dev-console",
    "start:dev-electron": "NODE_ENV=development electron -r babel-register -r babel-polyfill ./main",
    "start:dev-server": "NODE_ENV=development node -r babel-register ./webpack/server.js",
    "squad-updater": "node -r babel-register ./common/scraper-factory/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "adjective-animal": "1.1.3",
    "cheerio": "0.19.0",
    "cloudscraper": "1.5.0",
    "cuid": "2.1.1",
    "duel": "3.2.0",
    "duel-names": "1.2.0",
    "electron": "2.0.4",
    "empty": "0.10.1",
    "ffa": "3.2.2",
    "ffa-tb": "1.2.1",
    "font-awesome": "4.7.0",
    "glob": "7.1.2",
    "group": "1.0.2",
    "groupstage": "3.1.1",
    "groupstage-tb": "1.3.1",
    "groupstage-tb-duel": "1.2.0",
    "informed": "1.5.2",
    "lodash": "4.17.10",
    "masters": "3.1.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-particles-js": "2.2.0",
    "react-ratings-declarative": "3.4.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.4.0",
    "roundrobin": "1.0.3",
    "sequelize": "4.38.0",
    "sqlite3": "4.0.2",
    "tiebreaker": "3.1.0",
    "tournament": "3.2.0",
    "tourney": "1.2.1",
    "webpack-hot-middleware": "2.22.2"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.4.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "electron-rebuild": "1.8.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.0",
    "eslint-plugin-react": "7.10.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "flow-bin": "0.76.0",
    "flow-typed": "2.5.1",
    "jest": "23.3.0",
    "minimist": "1.2.0",
    "mock-fs": "4.5.0",
    "node-sass": "4.9.1",
    "postcss-loader": "2.1.5",
    "sass-loader": "7.0.3",
    "sequelize-cli": "4.0.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.15.1",
    "webpack-dev-middleware": "3.1.3"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "lcov",
      "json-summary"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|jpg|png)$": "<rootDir>/__test__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "common"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 67,
        "lines": 70,
        "statements": 70
      }
    },
    "globals": {
      "__DEV__": true
    },
    "notify": true,
    "collectCoverageFrom": [
      "common/**/*.{js,jsx}"
    ]
  }
}
