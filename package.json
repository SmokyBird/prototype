{
  "name": "la-liga",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "description": "A football manager-type game for e-sports",
  "homepage": "https://github.com/lemonpole/la-liga#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/la-liga.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/la-liga/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "e-sport",
    "simulator"
  ],
  "main": "main.js",
  "dependencies": {
    "electron": "1.7.11",
    "empty": "0.10.1",
    "font-awesome": "4.7.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router": "4.2.0",
    "webpack-hot-middleware": "2.18.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.1.0",
    "cheerio": "0.19.0",
    "cloudscraper": "1.4.1",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-flowtype-errors": "3.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "express": "4.15.4",
    "file-loader": "0.11.2",
    "flow-bin": "0.54.0",
    "flow-typed": "2.1.5",
    "glob": "7.1.2",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0"
  },
  "scripts": {
    "build:main": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/webpack-electron.js --progress --profile --colors",
    "flow": "flow",
    "flow-typed": "flow-typed install --overwrite",
    "lint": "eslint --ext .js,.jsx ./",
    "start": "NODE_ENV=production electron ./dist/main.bundle.js",
    "start:dev": "npm run start:dev-server -- --dev-electron",
    "start:dev-electron": "NODE_ENV=development electron -r babel-register -r babel-polyfill ./main",
    "start:dev-server": "NODE_ENV=development node -r babel-register ./webpack/server.js",
    "squad-updater": "node -r babel-register ./scripts/squad-updater/index.js"
  },
  "false": {}
}
