{
  "name": "la-liga",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "description": "A football manager-type game for e-sports",
  "homepage": "https://github.com/lemonpole/la-liga#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/la-liga.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/la-liga/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "e-sport",
    "simulator"
  ],
  "main": "main.js",
  "dependencies": {
    "adjective-animal": "1.1.3",
    "cheerio": "0.19.0",
    "cloudscraper": "1.4.1",
    "cuid": "2.1.1",
    "duel": "3.2.0",
    "duel-names": "1.2.0",
    "electron": "2.0.2",
    "empty": "0.10.1",
    "ffa": "3.2.2",
    "ffa-tb": "1.2.1",
    "font-awesome": "4.7.0",
    "glob": "7.1.2",
    "group": "1.0.2",
    "groupstage": "3.1.1",
    "groupstage-tb": "1.3.1",
    "groupstage-tb-duel": "1.2.0",
    "lodash": "4.17.4",
    "masters": "3.1.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router": "4.2.0",
    "roundrobin": "1.0.3",
    "tiebreaker": "3.1.0",
    "tournament": "3.2.0",
    "tourney": "1.2.1",
    "webpack-hot-middleware": "2.18.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.1.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.7",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-import-resolver-webpack": "0.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-flowtype-errors": "3.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "express": "4.15.4",
    "file-loader": "0.11.2",
    "flow-bin": "0.54.0",
    "flow-typed": "2.4.0",
    "jest": "22.4.3",
    "minimist": "1.2.0",
    "mock-fs": "4.5.0",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "lcov",
      "json-summary"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|jpg|png)$": "<rootDir>/__test__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "common"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 67,
        "lines": 70,
        "statements": 70
      }
    },
    "globals": {
      "__DEV__": true
    },
    "notify": true,
    "collectCoverageFrom": [
      "common/**/*.{js,jsx}"
    ]
  },
  "scripts": {
    "build:main": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/webpack-electron.js --progress --profile --colors",
    "flow": "flow",
    "flow-typed": "flow-typed install --overwrite",
    "lint": "eslint --ext .js,.jsx ./",
    "start": "NODE_ENV=production electron ./dist/main.bundle.js",
    "start:dev": "npm run start:dev-server -- --dev-electron",
    "start:dev-electron": "NODE_ENV=development electron -r babel-register -r babel-polyfill ./main",
    "start:dev-server": "NODE_ENV=development node -r babel-register ./webpack/server.js",
    "squad-updater": "node -r babel-register ./common/squad-updater/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "false": {}
}
